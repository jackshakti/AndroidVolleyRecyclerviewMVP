<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/AppDetail_main_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_70sdp"
        android:background="#FFFFFF">

        <RelativeLayout
            android:id="@+id/AppDetail_header_layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:background="#FFFFFF">

            <RelativeLayout
                android:id="@+id/AppDetail_back_layout"
                android:layout_width="@dimen/_45sdp"
                android:layout_height="match_parent"
                android:background="#00000000">

                <ImageView
                    android:layout_width="@dimen/_16sdp"
                    android:layout_height="@dimen/_16sdp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/ic_arrow"
                    tools:ignore="ContentDescription" />

            </RelativeLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_toEndOf="@+id/AppDetail_back_layout"
                android:layout_toRightOf="@+id/AppDetail_back_layout"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:text="@string/app_name"
                android:textSize="@dimen/_16sdp" />

        </RelativeLayout>

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/AppDetail_main_layout"
        android:fillViewport="true"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:layoutDirection="locale"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/AppDetailHeader_main_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="#FFFFFF">

                    <ImageView
                        android:id="@+id/AppDetail_project_owner_logo_imageView"
                        android:layout_width="@dimen/_55sdp"
                        android:layout_height="@dimen/_55sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginLeft="@dimen/_7sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:layout_marginBottom="@dimen/_15sdp"
                        android:scaleType="fitXY"
                        android:src="@drawable/ic_launcher_background"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/AppDetailHeader_projectOwner_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_8sdp"
                        android:layout_marginLeft="@dimen/_8sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:layout_toEndOf="@+id/AppDetail_project_owner_logo_imageView"
                        android:layout_toRightOf="@+id/AppDetail_project_owner_logo_imageView"
                        android:text="Title"
                        android:textSize="@dimen/_11sdp" />

                    <TextView
                        android:id="@+id/AppDetailHeader_projectOwner_details_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/AppDetailHeader_projectOwner_label"
                        android:layout_marginStart="@dimen/_8sdp"
                        android:layout_marginLeft="@dimen/_8sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginEnd="@dimen/_7sdp"
                        android:layout_marginRight="@dimen/_7sdp"
                        android:layout_marginBottom="@dimen/_15sdp"
                        android:layout_toEndOf="@+id/AppDetail_project_owner_logo_imageView"
                        android:layout_toRightOf="@+id/AppDetail_project_owner_logo_imageView"
                        android:text=""
                        android:textSize="@dimen/_13sdp" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/AppDetailHeader_projectStage_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#e0f2ff"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/AppDetailHeader_projectStage_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginLeft="@dimen/_7sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textSize="@dimen/_11sdp" />

                    <TextView
                        android:id="@+id/AppDetail_projectStage_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/AppDetailHeader_projectStage_label"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:layout_marginEnd="@dimen/_7sdp"
                        android:layout_marginBottom="@dimen/_10sdp"
                        android:text=""
                        android:textSize="@dimen/_13sdp" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/AppDetailHeader_projectName_main_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF">

                    <TextView
                        android:id="@+id/AppDetailHeader_projectName_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginLeft="@dimen/_7sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:text="Name"
                        android:textSize="@dimen/_11sdp" />

                    <TextView
                        android:id="@+id/AppDetailHeader_projectName_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/AppDetailHeader_projectName_label"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginEnd="@dimen/_7sdp"
                        android:text=""
                        android:textSize="@dimen/_13sdp" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/AppDetailHeader_projectName_textView"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:layout_marginBottom="@dimen/_14sdp"
                        android:background="#FFFFFF"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:id="@+id/AppDetailHeader_postedDate_main_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF">

                            <TextView
                                android:id="@+id/AppDetailHeader_projectName_date_label"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentLeft="true"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/_7sdp"
                                android:layout_marginLeft="@dimen/_7sdp"
                                android:alpha="0.9"
                                android:gravity="start"
                                android:text="posted date"
                                android:textSize="@dimen/_11sdp" />

                            <TextView
                                android:id="@+id/AppDetailHeader_projectName_date_textView"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_2sdp"
                                android:layout_marginLeft="@dimen/_2sdp"
                                android:layout_toEndOf="@+id/AppDetailHeader_projectName_date_label"
                                android:layout_toRightOf="@+id/AppDetailHeader_projectName_date_label"
                                android:alpha="0.8"
                                android:text=""
                                android:textSize="@dimen/_11sdp" />

                        </RelativeLayout>

                    </LinearLayout>


                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/AppDetailHeader_sStory_main_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF">

                    <TextView
                        android:id="@+id/AppDetailHeader_sStory_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginLeft="@dimen/_7sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:text="label"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/AppDetailHeader_sStory_details_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/AppDetailHeader_sStory_label"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginTop="@dimen/_6sdp"
                        android:layout_marginEnd="@dimen/_7sdp"
                        android:layout_marginBottom="@dimen/_15sdp"
                        android:alpha="0.6"
                        android:text=""
                        android:textSize="@dimen/_12sdp" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/AppDetailHeader_youtube_main_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_60sdp"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:background="#e8eced"
                    android:visibility="visible">

                    <ImageView
                        android:id="@+id/AppDetailHeader_youtube_imageView"
                        android:layout_width="@dimen/_80sdp"
                        android:layout_height="@dimen/_25sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:padding="@dimen/_5sdp"
                        android:scaleType="fitXY"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/AppDetail_youtube_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:textSize="@dimen/_10sdp" />

                </RelativeLayout>

                <RelativeLayout

                    android:id="@+id/AppDetailHeader_investors_main_layout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginStart="@dimen/_7sdp"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:layout_marginEnd="@dimen/_7sdp"
                    android:layout_marginBottom="@dimen/_20sdp"
                    android:background="#e0f2ff"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/AppDetailHeader_investors_imageView"
                        android:layout_width="@dimen/_40sdp"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:padding="@dimen/_5sdp"
                        android:scaleType="fitXY"
                        tools:ignore="ContentDescription" />

                    <TextView
                        android:id="@+id/AppDetail_investors_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_7sdp"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:layout_toEndOf="@+id/AppDetailHeader_investors_imageView"
                        android:layout_toRightOf="@+id/AppDetailHeader_investors_imageView"
                        android:text=""
                        android:textSize="@dimen/_13sdp" />

                    <TextView
                        android:id="@+id/AppDetail_investors_label"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/AppDetail_investors_textView"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:layout_toEndOf="@+id/AppDetailHeader_investors_imageView"
                        android:layout_toRightOf="@+id/AppDetailHeader_investors_imageView"
                        android:textSize="@dimen/_10sdp" />

                </RelativeLayout>

            </LinearLayout>


        </LinearLayout>
    </ScrollView>


</RelativeLayout>
